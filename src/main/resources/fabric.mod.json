{
  "schemaVersion": 1,
  "id": "liveoverflowaddon",
  "version": "${version}",
  "name": "Addon Template",
  "description": "An addon for the LiveOverflow server",
  "authors": [
    "Patsore"
  ],
  "contact": {
    "repo": "https://github.com/patsore/LiveOverflowAddon"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.patsore.liveoverflowaddon.Addon"
    ]
  },
  "mixins": [
    "addon.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
